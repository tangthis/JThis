<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tang.dao.TestUserDao">
	<resultMap id="resultTestUser" type="com.tang.po.TestUser">
		<id column="id" property="id" />
		<result column="uname" property="uname" />
		<result column="phone" property="phone" />
		<result column="birthday" property="birthday" />
	</resultMap>
	
	<sql id="testUserColumns">
		id,uname,phone,birthday
	</sql>
   
    <select id="findList" resultMap="resultTestUser">
		SELECT <include refid="testUserColumns"/> FROM test_user
  	</select>
</mapper>